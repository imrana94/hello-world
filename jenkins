pipeline {
agent any
tools {
maven 'maven'
}
stages{
stage('build'){
steps{
sh script: 'mvn clean package'
}
}
stage('Upload War To Nexus'){
steps{
nexusArtifactUploader artifacts:[
[
artifactId: 'maven-project',
classifier: '',
file: 'target/maven-project-1.0.0.war',
type: 'war'
]
],
credentialsId: 'Nexus',
groupId: 'com.example.maven-project',
nexusUrl: '18.116.72.128:8081',
nexusVersion: 'nexus3',
protocol: 'http'
repository: 'pipeline',
version: '1.0.0'
}
}
}
}
